
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <!-- theme meta -->
    <meta name="theme-name" content="quixlab" />

    <title>Student Registration System</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="images/favicon.png">
    <!-- Pignose Calender -->
    <link href="~/theme/plugins/pg-calendar/css/pignose.calendar.min.css" rel="stylesheet">
    <!-- Chartist -->
    <link rel="stylesheet" href="~/theme/plugins/chartist/css/chartist.min.css">
    <link rel="stylesheet" href="~/theme/plugins/chartist-plugin-tooltips/css/chartist-plugin-tooltip.css">
    <!-- Data Table -->
    <link href="~/lib/datatables/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet" />
    <!-- Sweet Alert -->
    <link href="~/lib/bootstrap-sweetalert/sweetalert.css" rel="stylesheet" />
    <!-- Toast Message -->
    <link href="~/lib/toastr.js/toastr.css" rel="stylesheet" />
    <!-- Ladda Animation -->
    <link href="~/lib/Ladda/ladda-themeless.min.css" rel="stylesheet" />
    <link href="~/lib/Ladda/ladda.min.css" rel="stylesheet" />


    <link href="~/lib/select2-4.1.0/dist/css/select2.min.css" rel="stylesheet" />

    <!-- Custom Stylesheet -->
    <link href="~/theme/css/style.css" rel="stylesheet">
    <style>
        /*--side bar hover---*/
        .nk-sidebar .metismenu > li.active > a {
            background: #eeeeee !important;
            color: #464a53;
        }

        .nk-sidebar .metismenu a:hover, .nk-sidebar .metismenu a:active, .nk-sidebar .metismenu a.active {
            text-decoration: none;
            background-color: #eeeeee !important;
        }
    </style>
</head>

<body style="overflow-y:scroll;">

    <!--******************* Preloader start ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--******************* Preloader end ********************-->
    <!--********************************** Main wrapper start ***********************************-->
    <div id="main-wrapper">

        <!--********************************** Nav header start ***********************************-->
        <div class="nav-header">
            <div class="brand-logo">
                <a href="index.html">
                    <b class="logo-abbr"><img src="~/theme/images/ucsm.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/theme/images/ucsm.png" alt=""></span>
                    <span class="brand-title">
                        @*<img src="~/theme/images/ucsm.png" alt="" width="20" height="20">*@
                        <span style="color:white;text-align:center;margin-bottom:5px;margin-left:3rem;font-weight:bolder;">U C S M</span>
                    </span>
                </a>
            </div>
        </div>
        <!--********************************** Nav header end ***********************************-->
        <!--********************************** Header start ***********************************-->
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
                @*<div class="header-left">
                    <div class="input-group icons">
                        <div class="input-group-prepend">
                            <span class="input-group-text bg-transparent border-0 pr-2 pr-sm-3" id="basic-addon1"><i class="mdi mdi-magnify"></i></span>
                        </div>
                        <input type="search" class="form-control" placeholder="Search Dashboard" aria-label="Search Dashboard">
                        <div class="drop-down animated flipInX d-md-none">
                            <form action="#">
                                <input type="text" class="form-control" placeholder="Search">
                            </form>
                        </div>
                    </div>
                </div>*@
                <div class="header-right">
                    <ul class="clearfix">
                      
                        <li class="icons">
                            <a asp-action="Index" asp-controller="LogIn" class="log-user" style="display:flex;align-items:center;">
                                <i class="bi bi-box-arrow-in-right" style="color: #000;"></i><span>Log Out</span>
                            </a>
                        </li>
                        
                    </ul>
                </div>
            </div>
        </div>
        <!--********************************** Header end ti-comment-alt ***********************************-->
        <!--********************************** Sidebar start ***********************************-->
        @*<div class="nk-sidebar" style="height:100vh">*@
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">

                    <li class="nav-label" style="font-weight:bold;">Dashboard</li>
                    <li>
                        <a class="" asp-action="Index" asp-controller="Home" aria-expanded="false">
                            <i class="icon-speedometer menu-icon"></i><span class="nav-text">Dashboard</span>
                        </a>
                    </li>

                    <li class="nav-label" style="font-weight:bold;">Transaction</li>
                    <li>
                        <a class="" aria-expanded="false" asp-action="Index" asp-controller="TimeTable">
                            <i class="bi bi-calendar3"></i> <span class="nav-text">Manage Timetable</span>
                        </a>
                    </li>
                    <li>
                        <a class="" aria-expanded="false" asp-action="Index" asp-controller="Result">
                            <i class="bi bi-file-earmark-text"></i><span class="nav-text">Manage Result</span>
                        </a>
                    </li>

                    <li class="nav-label" style="font-weight:bold;">Reports</li>
                    <li>
                        <a class="" aria-expanded="false" asp-action="StudentInfo" asp-controller="Report">
                            <i class="bi bi-person-badge"></i><span class="nav-text">Student Info Report</span>
                        </a>
                    </li>
                    <li>
                        <a class="" aria-expanded="false" asp-action="StudentCourseInfo" asp-controller="Report">
                            <i class="bi bi-journal-bookmark"></i><span class="nav-text">Student Course Report</span>
                        </a>
                    </li>
                    <li class="nav-label" style="font-weight:bold;">Management</li>
                    <li>
                        <a class="has-arrow" href="javascript:void()" aria-expanded="false">
                            <i class="icon-grid menu-icon"></i><span class="nav-text">System set up</span>
                        </a>
                        <ul aria-expanded="false">
                            <li><a asp-action="Index" asp-controller="Semester">Semester</a></li>
                            <li><a asp-action="Index" asp-controller="YearLevel">Year Level</a></li>
                            <li><a asp-action="Index" asp-controller="AcademicYear">Academic Year</a></li>
                            <li><a asp-action="Index" asp-controller="Section">Major</a></li>
                            <li><a asp-action="Index" asp-controller="Period">Period</a></li>
                            <li><a asp-action="Index" asp-controller="Prerequisites">Prerequisites</a></li>
                            <li><a asp-action="Index" asp-controller="Course">Course</a></li>
                            <li><a asp-action="TimeLimitIndex" asp-controller="Student">TimeLimit</a></li>
                        </ul>
                    </li>

                </ul>
            </div>
        </div>
        <!--********************************** Sidebar end ***********************************-->
        <!--********************************** Content body start ***********************************-->
        <div class="content-body">
            @RenderBody()
        </div>
    </div>
    <!--********************************** Main wrapper end ***********************************-->
    <!--********************************** Scripts ***********************************-->
    <script src="~/theme/plugins/common/common.min.js"></script>
    <script src="~/lib/theme-js/custom.min.js"></script>
    <script src="~/lib/theme-js/settings.js"></script>
    <script src="~/lib/theme-js/gleek.js"></script>
    <script src="~/lib/theme-js/styleSwitcher.js"></script>

    <!-- Circle progress -->
    <script src="~/theme/plugins/circle-progress/circle-progress.min.js"></script>
    <!-- Datamap -->
    <script src="~/theme/plugins/d3v3/index.js"></script>
    <script src="~/theme/plugins/topojson/topojson.min.js"></script>
    <script src="~/theme/plugins/datamaps/datamaps.world.min.js"></script>
    <!-- Morrisjs -->
    <script src="~/theme/plugins/raphael/raphael.min.js"></script>
    <script src="~/theme/plugins/morris/morris.min.js"></script>
    <!-- Pignose Calender -->
    <script src="~/theme/plugins/moment/moment.min.js"></script>
    <script src="~/theme/plugins/pg-calendar/js/pignose.calendar.min.js"></script>
    <!-- ChartistJS -->
    <script src="~/theme/plugins/chartist/js/chartist.min.js"></script>
    <script src="~/theme/plugins/chartist-plugin-tooltips/js/chartist-plugin-tooltip.min.js"></script>
    <!-- Dashboard JS -->
    <script src="~/theme/js/dashboard/dashboard-1.js"></script>
    <!-- JQuery and Bootstrap JS -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Custom JS -->
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- Data Tables JS -->
    <script src="~/lib/datatables/js/jquery.dataTables.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.js"></script>
    <!-- Sweet Alert JS -->
    <script src="~/lib/bootstrap-sweetalert/sweetalert.js"></script>
    <!-- Toast Message JS -->
    <script src="~/lib/toastr.js/toastr.min.js"></script>
    <!-- Ladda Animation JS -->
    <script src="~/lib/Ladda/spin.min.js"></script>
    <script src="~/lib/Ladda/ladda.min.js"></script>
    <script src="~/lib/Ladda/ladda.jquery.min.js"></script>

    <script src="~/lib/select2-4.1.0/dist/js/select2.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
